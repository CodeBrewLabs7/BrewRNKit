{
  "name": "react-native-unistyles",
  "version": "2.4.0",
  "summary": "Level up your React Native StyleSheet",
  "homepage": "https://github.com/jpudysz/react-native-unistyles#readme",
  "license": "MIT",
  "authors": "Jacek Pudysz <jacekpudysz@gmail.com> (https://github.com/jpudysz)",
  "platforms": {
    "ios": "13.4",
    "osx": "10.14"
  },
  "source": {
    "git": "https://github.com/jpudysz/react-native-unistyles",
    "tag": "2.4.0"
  },
  "source_files": [
    "ios/**/*.{h,mm}",
    "cxx/*.{h,cpp}"
  ],
  "compiler_flags": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [

    ],
    "RCT-Folly": [
      "2022.05.16.00"
    ],
    "glog": [

    ]
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  }
}
